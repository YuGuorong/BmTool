<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<resource id="" version="1.0">
    <!-- meta 元素可以有多个，以下这些属性可以分开设置，属性可剪裁 -->
        <meta uuid="!&entry"  caption="!&title"  author="!&contributor"
		keywords="!&subject_keyword" tag="" />
        
    <!--BEGIN: 北京资源网专用-->
        <scheme id="" caption="">
			<section id="" caption=""/>
        </scheme>
        <school id="" caption=""/>
        <edition id="" caption="!&textbook_release"/>
        <book id="" caption="" />
        <schoolYear id="" caption="" beginDate="" endDate=""/>
        <!-- 所属图书的目录项-->
        <bookCatalog>                
            !&dirs
        </bookCatalog>
        <targetUser id="" value="!&audience"/>
        <resourceKind id="!&metadataclass" value=""/>
    <!--END: 北京资源网专用-->
              

    <!--适用年级,可存在多个年级-->
        <grade>
            !&grades
        </grade>
        
    <!-- 如果学科是树形结构，可以多级嵌套-->
        <subject>
            <item id="" caption="!&subject_caption">
            </item>
        </subject>

    <!-- 如果分类是树形结构，可以多级嵌套-->

        <scope id="" value="!&courseclass"/>
        <press id="" caption="!&press"/>

	<!--版权相关-->
        <copyrights owner="!&copyright" provider="!&release"/>                

    <!-- meta 元素可以有多个，以下这些属性可以分开设置，属性可剪裁 -->
        <meta isbn="!&isbn" publishDate="!&date" pageCount="!&pages" wordCount="!&size"
                language="!&language"
                bookPrice="!&bookprice" ebookPrice="!&price" salePrice=""
                publishString="!&meta-meta_data"/>


    <!--一下属性可作为数据交换用-->
        <meta viewCount="" downloadCount="" favoriteCount="" previewCount="" appraiseCount="" complainCount="" />
        <meta score1="" score2="" score3="" score4="" score5="" score6="" score7="" score8="" score9="" />

    <!--以下属性作为大段文本出现，在系统设计中，可能会将该属性作为一个单据的记录出现，故可能存在ID，在某些系统中，如果没有ID，可以省略。-->
        <preface id=""></preface>
        <summary id="">!&description</summary>
        <authBrief id=""></authBrief>
        <digest id=""></digest>
        <recommend id=""></recommend>
        <comment id=""></comment>

        <files>
			!&cover
!&files
        </files>

    <!--操作者定义，可根据需要剪裁-->
        <operator>
                !&operator
                <checker id="" caption="" time="" state="">
                        <!--审核意见可以放在此处-->
                </checker>
        </operator> 
</resource>